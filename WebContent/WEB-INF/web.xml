<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>TaskCity</display-name>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>404</servlet-name>
		<jsp-file>/404.html</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>404</servlet-name>
		<url-pattern>/404</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>faqs</servlet-name>
		<jsp-file>/faqs.html</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>faqs</servlet-name>
		<url-pattern>/faqs</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>report</servlet-name>
		<jsp-file>/report.html</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>report</servlet-name>
		<url-pattern>/report</url-pattern>
	</servlet-mapping>
</web-app>
